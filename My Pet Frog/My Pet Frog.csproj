<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>My_Pet_Frog</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Typewriter.dll" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Typewriter.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>


  <ItemGroup>
    <Reference Include="Typewriter">
      <HintPath>Typewriter.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
